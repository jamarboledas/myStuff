{% extends '::base.html.twig' %}

{% block body %}

    <table class="table table-striped table-hover" style="width: 300px;" align="center">
        <thead>
        <tr>
            <th>id</th>
            <th>name</th>
        </tr>
        </thead>
        <tbody>
            {% for program in programs %}
            <tr>
                <td>{{ program.id }}</td>
                <td>{{ program.name }}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>

    <div align="center">
        <ul class="pagination">
            {% for i in 1..num_pags %}
            <li><a href={{ path('test_program_indexdql', {'first': (i-1)*5}) }}>{{ i }}</a></li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}